{
  "name": "skovhus.github.io",
  "private": true,
  "description": "Personal website and blog by Kenneth Skovhus",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "deploy": "yarn verify && yarn build && gh-pages --dist public --branch master --message 'Deploy to GitHub pages'",
    "lint": "eslint . --ext js,ts,tsx --fix && prettier src/**/*.css --write",
    "typecheck": "tsc --noEmit",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "verify": "yarn lint && yarn typecheck && yarn test"
  },
  "dependencies": {
    "@babel/core": "7.4.5",
    "gatsby": "^2.9.4",
    "gatsby-image": "^2.1.4",
    "gatsby-plugin-feed": "^2.2.3",
    "gatsby-plugin-google-analytics": "^2.1.4",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^2.1.3",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.1.5",
    "gatsby-plugin-typescript": "^2.0.15",
    "gatsby-plugin-typography": "^2.2.13",
    "gatsby-remark-copy-linked-files": "^2.0.13",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.0.16",
    "gatsby-remark-prismjs": "^3.2.11",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.39",
    "gatsby-transformer-remark": "^2.4.0",
    "gatsby-transformer-sharp": "^2.1.21",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typeface-merriweather": "^0.0.72",
    "typeface-montserrat": "^0.0.75",
    "typescript": "^3.5.3",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "@types/react-helmet": "^5.0.9",
    "@types/typography": "^0.16.2",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "gh-pages": "^2.1.1",
    "prettier": "^1.18.2"
  }
